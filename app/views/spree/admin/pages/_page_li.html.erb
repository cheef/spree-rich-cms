<li class="page">
  <div class="alpha five columns page-slug">
    <%= link_to spree_page_path(page), target: '_blank' do %>
      <%= spree_page_path(page) %><i class="icon-external-link"></i>
    <% end %>
  </div>
  <h3 class="seven columns page-title">
    <%= link_to edit_admin_page_path(page.id) do%>
      <%= page.name %><i class="icon-edit"></i>
    <% end %>
  </h3>
</li>